{
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },

  "rules": {
    "no-console": 0,
    "indent": ["warn", 2, {"SwitchCase": 1}],
    "semi": [2, "always"],
    "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }]
  }
}
